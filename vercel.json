{
  "build": {
    "env": {
      "BUN_VERSION": "1.2.1"
    }
  },
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "functions": {
    "api/index.ts": {
      "runtime": "vercel-bun@1.0.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
