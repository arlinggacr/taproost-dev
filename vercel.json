{
  "build": {
    "env": {
      "BUN_VERSION": "1.2.1"
    }
  },
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "functions": {
    "api/main.ts": {
      "runtime": "bun@1.2.1"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.ts"
    }
  ]
}
