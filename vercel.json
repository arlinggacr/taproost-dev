{
  "build": {
    "env": {
      "BUN_VERSION": "1.2.1"
    }
  },
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "builds": [
    {
      "src": "src/main.ts"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
